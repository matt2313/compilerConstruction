/* This code shows basic arithmetic on ints, and how to use both types of comments */

int main()
{
    -1;
    0;
    1;
    // Comment
    1 + 1; // 2
    5 - 2; // 3
    /*
    Multiline Comment
    */
    16 / 4; // 4
    5 * 1; // 5
    2 + 2 * 2; // 6
    /*
     *
     *  Fancy
     *  Comment
     *
     */
    (10 - 2) /* You can even comment here */ - 1; //7
    -2 * ((6 - 2) + ((0 - 2) * (4)));//8
    3 + 3 + 3; // 9
    
    return 0;
}