/* This code shows basic operations on bools */

true; // true
false; // false
true OR false; // true
false OR false; // false
true OR false OR false OR false OR true; // true
true AND false AND false AND true; // false
NOT false; // true
(5 < 5) NXOR (5 == 5); // false
false NOR (true XOR true); // true
10 > 8 NAND 100 <= (200 - 100); // false
(17 != 16) OR (12 != 12); // true
true != true; // false
true == true; // true
false <> false; // false